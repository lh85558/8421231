config cupsd 'global'
	option enabled '1'
	option port '631'
	option listen_addr '192.168.10.1'
	option web_interface '1'
	option remote_admin '1'
	option remote_any '1'
	option share_printers '1'
	option browse_address '@LOCAL'
	option browse_order 'allow,deny'
	option browse_allow '@LOCAL'
	option browse_local_protocols 'dnssd'
	option browse_remote_protocols 'dnssd'
